include ../shopListItem/shopListItem

mixin shopListMain(data)
    - var shopListMainData = data ? data : {};

    - shopListMainData.list = shopListMainData.list ? shopListMainData.list : [];

    +b('section').shopListMain
        +e('div').wrapper
            +e('div').content

            +e('div').footer
                +e('div').template(id="shopListItem")
                    +shopListItem

                button.shopListMain__add
